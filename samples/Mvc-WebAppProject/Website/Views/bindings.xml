    <end>#}</end>
  </element>
  <element name="ScriptBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.ScriptRegistry())) {</start>
    <end>#}</end>
  </element>
  <element name="StyleBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.StyleRegistry('@target'))) {</start>
    <end>#}</end>
  </element>
