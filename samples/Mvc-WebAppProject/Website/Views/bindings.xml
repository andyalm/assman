<?xml version="1.0" encoding="utf-8" ?>
<bindings>
  <element name="ScriptBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.ScriptRegistry('@target'))) {</start>
    <end>#}</end>
  </element>
  <element name="ScriptBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.ScriptRegistry())) {</start>
    <end>#}</end>
  </element>
  <element name="StyleBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.StyleRegistry('@target'))) {</start>
    <end>#}</end>
  </element>
  <element name="StyleBlock">
    <start>#using(new InlineBlockOutputScope(this, Html.StyleRegistry())) {</start>
    <end>#}</end>
  </element>
  <element name="ScriptInclude">#Html.ScriptRegistry('@target').IncludePath('@src');</element>
  <element name="ScriptInclude">#Html.ScriptRegistry().IncludePath('@src');</element>
  <element name="StyleInclude">#Html.StyleRegistry('@target').IncludePath('@src');</element>
  <element name="StyleInclude">#Html.StyleRegistry().IncludePath('@src');</element>
</bindings>