<?xml version="1.0"?>
<almWitt.web.resourceManagement consolidate="true">
  <clientScripts minify="true">
    <group consolidatedUrl="~/scripts/release/default.js">
      <exclude>
        <add pattern="/Secondary"/>
        <add pattern="/ExcludedScript.js"/>
      </exclude>
    </group>
    <group consolidatedUrl="~/scripts/release/secondary.js">
      <include>
        <add pattern="/Script1.js"/>
        <add pattern="/Secondary1.js"/>
      </include>
    </group>
  </clientScripts>
  <assemblies>
    <add name="WebsiteLib"/>
  </assemblies>
</almWitt.web.resourceManagement>