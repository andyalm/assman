<?xml version="1.0"?>
<almWitt.web.resourceManagement consolidate="true">
  <cssFiles>
    <group consolidatedUrl="~/ConsolidatedStyles/All.css">
      <include>
        <add pattern="~/Styles/.+\.css"/>
        <add pattern="~/Styles/.+\.less"/>
      </include>
    </group>
  </cssFiles>
  <customResourceFinders>
    <add factory="AlmWitt.Web.ResourceManagement.Less.LessResourceFinderFactory, AlmWitt.Web.ResourceManagement.Less, Version=1.0.0.0, Culture=neutral" />
  </customResourceFinders>
</almWitt.web.resourceManagement>
