    <tags>javascript css mash minify consolidation yui yuicompressor</tags>
    <dependencies>
      <dependency id="Assman.Core" version="1.9" />
      <dependency id="YUICompressor.NET" version="1.5.0.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="NuGet\lib\Assman.YuiCompressor.dll" target="lib" />
    <file src="NuGet\Content\**\*.*" target="Content" />
