<?xml version="1.0"?>
<package xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <id>Assman.YuiCompressor</id>
    <version>0.0</version>
    <title>Assman YUI Compressor Integration</title>
    <authors>Andy Alm, Greg Musick</authors>
    <licenseUrl>http://www.opensource.org/licenses/bsd-license.php</licenseUrl>
    <projectUrl>http://assman.codeplex.com</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
