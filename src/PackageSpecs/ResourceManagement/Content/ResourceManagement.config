<almWitt.web.resourceManagement consolidate="true" manageDependencies="true">
  <clientScripts>
    <group consolidatedUrl="~/Scripts/Consolidated/Shared.js">
      <include>
        <add pattern="~/Scripts/jquery-1\.\d\.\d\.min\.js" mode="Release" />
        <add pattern="~/Scripts/jquery-1\.\d\.\d\.js" mode="Debug" />
      </include>
    </group>
  </clientScripts>
  <cssFiles>
    <group consolidatedUrl="~/Content/Consolidated/Shared.css">
      <include>
        <add pattern="~/Content/.+" />
      </include>
    </group>
  </cssFiles>
</almWitt.web.resourceManagement>