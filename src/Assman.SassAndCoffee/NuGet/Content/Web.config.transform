<configuration>
	<system.web>
		<httpHandlers>
			<add verb="GET,HEAD" path="*.sass" validate="false" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
			<add verb="GET,HEAD" path="*.scss" validate="false" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
			<add verb="GET,HEAD" path="*.coffee" validate="false" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
		</httpHandlers>
	</system.web>
	<system.webServer>
		<handlers>
			<add name="ConsolidatedSassHandler" preCondition="integratedMode" verb="GET,HEAD" path="*.sass" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
			<add name="ConsolidatedScssHandler" preCondition="integratedMode" verb="GET,HEAD" path="*.scss" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
			<add name="ConsolidatedCoffeeScriptHandler" preCondition="integratedMode" verb="GET,HEAD" path="*.coffee" type="Assman.Handlers.ResourceHttpHandlerFactory, Assman, Version=1.0.0.0, Culture=neutral"/>
		</handlers>
	</system.webServer>
</configuration>