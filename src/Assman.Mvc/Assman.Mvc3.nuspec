    <tags>javascript css mash consolidation mvc</tags>
    <dependencies>
      <dependency id="Assman.Core" version="1.9" />
      <dependency id="NuGetPowerTools" version="0.26" />
    </dependencies>
  </metadata>
  <files>
    <file src="NuGet\lib\Mvc3\**\Assman.Mvc.dll" target="lib" />
